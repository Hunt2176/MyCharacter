<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="root" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label fx:id="gaugeName" text="Label" />
      <ProgressBar fx:id="gaugeBar" progress="0.0" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS" />
      <GridPane GridPane.rowIndex="2">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button fx:id="gaugeAdd" alignment="CENTER" layoutX="31.0" layoutY="2.0" mnemonicParsing="false" text="+" AnchorPane.leftAnchor="34.0" AnchorPane.topAnchor="4.0" />
                  <Button fx:id="gaugeSub" alignment="CENTER" layoutX="4.0" layoutY="4.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" text="-" AnchorPane.leftAnchor="4.0" AnchorPane.topAnchor="4.0" />
               </children>
            </AnchorPane>
         </children>
      </GridPane>
   </children>
</GridPane>
